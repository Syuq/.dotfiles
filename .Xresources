Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! window padding
/* st.borderpx: 12 */

/* *background: #24283b */
/* *foreground: #c0caf5 */
