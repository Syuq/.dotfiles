st.foreground: #c0caf5

st.fading:                          8
st.fadeColor:                       black
st.cursorColor:                     #E4E4E4	
!!*cursorColor:                     #6df702	
st.pointerColorBackground:          #2B2C2B
st.pointerColorForeground:          #16A085

!! Transparency (0-1):
